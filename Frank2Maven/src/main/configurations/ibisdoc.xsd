    <xs:attribute name="certificateAuthAlias" type="xs:string">
      <xs:annotation>
        <xs:documentation>41 (default: alias used to obtain certificate password)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="certificatePassword" type="xs:string">
      <xs:annotation>
        <xs:documentation>42 (default: certificate password)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="charSet" type="xs:string">
      <xs:annotation>
        <xs:documentation>6 (default: charset of the request. Typically only used on PUT and POST requests.)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="contentType" type="xs:string">
      <xs:annotation>
        <xs:documentation>4 (default: content-type (superset of mimetype + charset) of the request, for POST and PUT methods)</xs:documentation>
