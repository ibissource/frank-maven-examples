      <xs:annotation>
        <xs:documentation>Certificate Password (default: )</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="certificateUrl" type="xs:string">
      <xs:annotation>
        <xs:documentation>Path (or resource url) to certificate to be used for authentication (default: )</xs:documentation>
